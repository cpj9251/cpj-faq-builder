{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "cpj-faq-builder/cpj-faq-builder",
	"version": "0.1.0",
	"title": "CPJ FAQ Builder",
	"category": "widgets",
	"icon": "smiley",
	"description": "This block creates a list of faqs with answers to be displayed when clicked",
	"supports": {
		"html": true,
		"color":{
			"background":true,
			"link":true,
			"text":true
		},
		"typography":{
			"fontSize":true,
			"lineHeight": true
		}
	},
	"textdomain": "cpj-faq-builder",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes":{
		"content":{
			"type":"string",
			"source":"html",
			"selector":"p"
		}
	}
}
